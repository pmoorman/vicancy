<div class="col-xs-8" style="padding-left: 0px; padding-right: 0px">
  <iframe
  {{bind-attr src=vimeo_url}} width='100%' height='307' frameborder='0' webkitallowfullscreen mozallowfullscreen
  allowfullscreen></iframe>
  <!--<div class="frame">-->
  <!--<img {{bind-attr src=youtube_preview_url}} width='100%' height='307'>-->
  <!--</div>-->
  <div class="frame_desc">
    <div class="row">
      <div class="col-xs-12">
        <div class="desc">{{ job_title }}</div>
        <div>
          <small class="text-muted">{{ company }}</small>
        </div>
      </div>
    </div>
    <div class="row">
      <div class="col-xs-2">
        <div class="icon-btn text-center">
          <i class="fa fa-edit"></i>

          <div>Edit</div>
        </div>
      </div>
      <div class="col-xs-2">
        <div class="icon-btn text-center">
          <i class="fa fa-trash-o"></i>

          <div>Delete</div>
        </div>
      </div>
      <div class="col-xs-2">
        <div class="icon-btn text-center" {{action 'share'}}>
          <i class="fa fa-share-square-o"></i>

          <div>Share</div>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="col-xs-8" id="sidr">
  Share
</div>
